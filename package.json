{
    "name": "python-prev",
    "displayName": "Python Prev",
    "description": "Line Preview for Python",
    "publisher": "ClemCa",
    "version": "0.0.27",
    "engines": {
      "vscode": "^1.87.0"
    },
    "icon": "icon.png",
    "categories": [
      "Other"
    ],
    "contributes": {
      "languages": [
        {
          "id": "python",
          "aliases": ["Python", "python"],
          "extensions": [".py"],
          "configuration": "./language-configuration.json"
        }
      ],
      "configuration": {
        "type": "object",
        "title": "Python Prev",
        "properties": {
          "python-prev.color": {
            "type": "string",
            "default": "#FFFFFF20",
            "description": "Default color for preview lines"
          },
          "python-prev.activeColor": {
            "type": "string",
            "default": "#FFFFFF88",
            "description": "Color of the active preview line"
          },
          "python-prev.error": {
            "type": "string",
            "default": "#FF000088",
            "description": "Default color for errors"
          },
          "python-prev.activeError": {
            "type": "string",
            "default": "#FF0000AA",
            "description": "Color of the active preview error"
          },
          "python-prev.timeout": {
            "type": "number",
            "default": 1000,
            "description": "Time in ms to wait for execution before declaring a timeout"
          }
        }
      }
    },
    "activationEvents": [],
    "main": "./out/extension.js",
    "repository": {
      "type": "git",
      "url": "https://github.com/ClemCa/python-prev"
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "vscode-test"
    },
    "devDependencies": {
      "@types/vscode": "^1.87.0",
      "@types/mocha": "^10.0.6",
      "@types/node": "18.x",
      "@typescript-eslint/eslint-plugin": "^7.0.2",
      "@typescript-eslint/parser": "^7.0.2",
      "eslint": "^8.56.0",
      "typescript": "^5.3.3",
      "@vscode/test-cli": "^0.0.6",
      "@vscode/test-electron": "^2.3.9"
    }
  }